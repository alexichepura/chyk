{
  "author": "Palessit",
  "description": "Chyk is a microframework to build universal SPAs with React.",
  "devDependencies": {
    "@types/history": "^4.7.7",
    "@types/node": "^14.6.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-config": "^5.0.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack": "^4.41.21",
    "@types/webpack-dev-server": "^3.11.0",
    "dotenv": "^8.2.0",
    "history": "^4.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.2.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.61",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "files": [
    "dist/*(*.ts|*.js|*.map)",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "chyk",
  "peerDependencies": {
    "history": "4.x",
    "react": "16.x",
    "react-dom": "16.x",
    "react-router": "5.x",
    "react-router-config": "5.x",
    "react-router-dom": "5.x"
  },
  "repository": "github:palessit/chyk",
  "keywords": [
    "microframework",
    "universal",
    "SPA",
    "SSR"
  ],
  "bugs": {
    "url": "https://github.com/palessit/chyk/issues"
  },
  "homepage": "https://github.com/palessit/chyk",
  "scripts": {
    "build": "tsc",
    "clean": "rm -r dist",
    "example:server": "cd example && ts-node-dev --respawn --transpile-only server.ts",
    "example:wds": "cd example && TS_NODE_PROJECT=\"tsconfig.json\" webpack-dev-server --config webpack.config.ts",
    "pack:dry": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
