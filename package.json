{
  "author": "Alexi Chepura",
  "description": "Chyk is a microframework to build universal SPAs with React.",
  "devDependencies": {
    "@types/history": "^4.7.7",
    "@types/node": "^14.14.10",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-config": "^5.0.0",
    "@types/react-router-dom": "^5.1.5",
    "dotenv": "^8.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.4",
    "history": "^4.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.2.0",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "files": [
    "dist/*(*.ts|*.js|*.map)",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "chyk",
  "peerDependencies": {
    "history": "4.x",
    "react": "*",
    "react-dom": "*",
    "react-router": "*",
    "react-router-config": "*",
    "react-router-dom": "*"
  },
  "repository": "github:palessit/chyk",
  "keywords": [
    "microframework",
    "universal",
    "SPA",
    "SSR"
  ],
  "bugs": {
    "url": "https://github.com/palessit/chyk/issues"
  },
  "homepage": "https://github.com/palessit/chyk",
  "scripts": {
    "build": "tsc",
    "clean": "rm -r dist",
    "example:server": "cd example && ts-node-dev --respawn --transpile-only server.tsx",
    "wds": "cd example && dotenv -- webpack s --config webpack.config.js",
    "pack:dry": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz"
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
